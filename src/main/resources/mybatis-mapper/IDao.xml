<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper
   PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
   "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
   
<mapper namespace="com.joonyoung.test.dao.IDao">
	<insert id="memberJoin">
		INSERT INTO member(mid, mpw, mname, memail) VALUES(#{param1}, #{param2}, #{param3}, #{param4})
	</insert>
	
	<select id="memberList" resultType="com.joonyoung.test.dto.MemberDto">
		SELECT * FROM member
	</select>
	
	<select id="searchIdOk" resultType="com.joonyoung.test.dto.MemberDto">
		SELECT * FROM member where mid = #{param1}
	</select>
</mapper>